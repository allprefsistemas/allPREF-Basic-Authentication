<?php

namespace AllPref\Entities;

/**
 * @Entity
 * @Table(name="user")
 */
class User
{
    /**
     * @Id
     * @GeneratedValue(strategy="AUTO")
     * @Column(type="integer")
     */
    private $id;

    /**
     * @OneToOne(targetEntity="UserCredentials")
     * @JoinColumn(name="id_usercredentials", referencedColumnName="id")
     */
    private $usercredentials;

    /**
     * @Column(type="string")
     */
    private $nome;

    /**
     * @Column(type="string")
     */
    private $telefones;

    /**
     * @Column(type="string")
     */
    private $endereco;

    /**
     * @Column(type="string")
     */
    private $bairro;

    /**
     * @Column(type="string")
     */
    private $cidade;

    /**
     * @Column(type="string")
     */
    private $estado;
}
